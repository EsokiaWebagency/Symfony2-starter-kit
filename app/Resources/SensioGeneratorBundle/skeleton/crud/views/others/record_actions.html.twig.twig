{{ "{% block content_footer -%}" }}


<ul class="list-inline">
    <li>
        <a href="{{ "{{ path('" ~ route_name_prefix ~ "') }}" }}"  class="btn btn-default">
           {{ " {{'esokia.back_to_list'|trans({}, 'esokia_default')}} " }}
        </a>
    </li>
{% if ('edit' in actions) and (not hide_edit) %}
    <li>
        <a href="{{ "{{ path('" ~ route_name_prefix ~ "_edit', { 'id': entity.id }) }}" }}">
                  {{ " {{'esokia.edit'|trans({}, 'esokia_default')}} " }}
        </a>
    </li>
{% endif %}
{% if ('delete' in actions) and (not hide_delete) %}
    <li>{{ '{{ form(delete_form) }}' }}</li>
{% endif %}
</ul>
{{ "{% endblock content_footer -%}" }} 



